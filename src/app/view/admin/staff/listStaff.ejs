<div style="width: 100%;height: 4rem;background-color: white; position: fixed; z-index: 1; box-shadow: 0 2px 5px gray">
    <nav class="navbar">
        <div class="container-fluid">
            <div class="d-flex" id="search_box_product" role="search">
                <a href="/supper-admin/staff/add"><button class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#new_product_model" style="margin-left: 2rem">Add staff</button></a>
            </div>
        </div>
    </nav>
</div>
<div style="padding-top: 5rem">
    <table class="table table-hover pt-10">
        <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Phone</th>
            <th scope="col">shop manager</th>
            <th scope="col">Action</th>
        </tr>
        </thead>
        <tbody>
        <% for (let i=0; i< staffList.length;i++){ %>
            <tr>
                <th scope="row"><%= i + 1 %></th>
                <td><%= staffList[i].fullname %></td>
                <td><%= staffList[i].phone %></td>
                <td><%= staffList[i].shop_name %></td>
                <td>
                    <div class="btn-group" role="group">
                        <a href="/supper-admin/staff/edit/<%= staffList[i].user_id %>"><button type="button" class="btn btn-primary">Edit</button></a>
                    </div>
                </td>
            </tr>
        <% } %>
        </tbody>
    </table>
</div>